<div>
	<%= form.label :title %>
	<%= form.text_field :title, :required => "true", :autocomplete => "off" %>
</div>

<div class="mt-3">
	<%= form.label :description %>
	<%= form.text_field :description, :required => "true", :autocomplete => "off" %>
</div>

<div class="mt-3">
  <%= form.label :rating %>
  <%= form.text_field :stars, :required => "true", :autocomplete => "off" %>
</div>

<div class="rating">
	<input type="radio" name="star" id="star-1"><label for="star-1"></label>
	<input type="radio" name="star" id="star-2"><label for="star-2"></label>
	<input type="radio" name="star" id="star-3"><label for="star-3"></label>
	<input type="radio" name="star" id="star-4"><label for="star-4"></label>
	<input type="radio" name="star" id="star-5"><label for="star-5"></label>
</div>
